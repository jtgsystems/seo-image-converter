{
  "name": "jtg-ai-image-converter",
  "version": "1.0.0",
  "description": "A SvelteKit project",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:web": "vite build",
    "build:tauri": "vite build && tauri build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "tauri:dev": "tauri dev",
    "tauri:check": "which tauri && pkg-config --exists libsoup-2.4 javascriptcoregtk-4.0 && echo 'Tauri dependencies OK' || echo 'Missing Tauri dependencies - web-only mode available'"
  },
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.53.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tauri-apps/api": "^1.5.3",
    "playwright": "^1.55.0",
    "svelte": "^4.2.0",
    "svelte-preprocess": "^5.1.0",
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@tauri-apps/cli": "^1.5.11",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.2",
    "postcss": "^8.4.31",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "terser": "^5.44.0"
  },
  "overrides": {
    "cookie": ">=0.7.0",
    "esbuild": ">=0.24.3"
  }
}
