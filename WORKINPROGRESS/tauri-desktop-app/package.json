{
  "name": "jtg-ai-image-converter",
  "version": "1.0.0",
  "description": "A SvelteKit project",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:web": "vite build",
    "build:tauri": "vite build && tauri build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "tauri:dev": "tauri dev",
    "tauri:check": "which tauri && pkg-config --exists libsoup-2.4 javascriptcoregtk-4.0 && echo 'Tauri dependencies OK' || echo 'Missing Tauri dependencies - web-only mode available'"
  },
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tauri-apps/api": "^2.9.1",
    "playwright": "^1.58.1",
    "svelte": "^5.49.1",
    "svelte-preprocess": "^6.0.3",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tauri-apps/cli": "^2.9.6",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "postcss": "^8.4.31",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "sass": "^1.97.3",
    "tailwindcss": "^4.1.18",
    "terser": "^5.46.0"
  },
  "overrides": {
    "cookie": ">=0.7.0",
    "esbuild": ">=0.24.3"
  }
}
